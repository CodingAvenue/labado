<!DOCTYPE html>
<html>
    <head>

        <title>{% block title %}Welcome!{% endblock %}</title>

        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <meta name="description" content="materialize is a material design based mutipurpose responsive template">
        <meta name="keywords" content="material design, card style, material template, portfolio, corporate, business, creative, agency">
        <meta name="author" content="trendytheme.net">

        <link rel="manifest" href="{{asset('manifest.json')}}">

        <link href='https://fonts.googleapis.com/css?family=Raleway:400,300,500,700,900' rel='stylesheet' type='text/css'>
        <!-- Material Icons CSS -->
        <link href="{{ asset('new-theme/fonts/iconfont/material-icons.css') }}" rel="stylesheet">
        <!-- FontAwesome CSS -->
        <link href="{{ asset('new-theme/fonts/font-awesome/css/font-awesome.min.css') }}" rel="stylesheet">
        <!-- flexslider -->
        <link href="{{ asset('new-theme/flexSlider/flexslider.css') }}" rel="stylesheet">
        <!-- materialize -->
        <link href="{{ asset('new-theme/materialize/css/materialize.min.css') }}" rel="stylesheet">
        <!-- Bootstrap -->
        <link href="{{ asset('new-theme/bootstrap/css/bootstrap.min.css') }}" rel="stylesheet">
        <!-- shortcodes -->
        <link href="{{ asset('new-theme/css/shortcodes/shortcodes.css') }}" rel="stylesheet">
        <!-- Style CSS -->
        <link href="{{ asset('new-theme/style.css') }}" rel="stylesheet">

        <!-- RS5.0 Main Stylesheet -->
        <link rel="stylesheet" type="text/css" href="{{ asset('new-theme/revolution/css/settings.css') }}">
        <!-- RS5.0 Layers and Navigation Styles -->
        <link rel="stylesheet" type="text/css" href="{{ asset('new-theme/revolution/css/layers.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('new-theme/revolution/css/navigation.css') }}">


        <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
        <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
        <!--[if lt IE 9]>
          <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js') }}"></script>
          <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js') }}"></script>
        <![endif]-->

        <link rel="icon" sizes="192x192" href="{{asset('img/LabadoLogo.png')}}">

        {% block stylesheets %}{% endblock %}

        <link rel="stylesheet" href="{{ asset('css/main.css') }}" media="screen,projection" />
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        <!--header start-->
        <header id="header" class="tt-nav navbar-border-bottom">

            <div class="header-sticky light-header ">

                <div class="container">

                    <div id="materialize-menu" class="menuzord">

                        <!--logo start-->
                        <a href="{{ path('homepage') }}" class="logo-brand">
                            <img class="retina" src="{{ asset('new-theme/img/logo.png') }}" alt=""/>
                        </a>
                        <!--logo end-->

                        <!--mega menu start-->
                        <ul class="menuzord-menu pull-right">

                            <li>
                                <a href="{{ path('homepage') }}">Home</a>
                            </li>

                            <li>
                                <a href="#">About</a>
                            </li>

                            <li>
                                <a href="javascript:void(0)">Contact</a>
                            </li>

                            {% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}
                                <li class="hide-on-med-and-down">
                                    <a class="fa fa-user" aria-hidden="true">
                                        {{app.user.username}}
                                    </a>
                                    <!-- Dropdown Structure -->
                                    <ul class='dropdown'>
                                        <li><a href="{{ path('fos_user_profile_show') }}">My Profile</a></li>
                                        <li><a href="{{ path('fos_user_change_password') }}">Change Password</a></li>
                                        <li class="divider"></li>
                                        <li><a href="{{ path('fos_user_security_logout') }}">logout</a></li>
                                    </ul>
                                </li>
                            {% else %}
                                <li class="hide-on-med-and-down"><a href="{{ path('fos_user_security_login') }} ">{{'layout.login'|trans({},'FOSUserBundle') }}</a></li>
                                <li class="hide-on-med-and-down"><a href="{{ path('fos_user_registration_register') }} ">{{'layout.register'|trans({},'FOSUserBundle') }}</a></li>
                            {% endif %}

                            <li>
                                <a class="blue white-text fa fa-book" aria-hidden="true" href="{{ path('search') }}" >
                                    Book Now
                                </a>
                            </li>
                        </ul>

                        <!--mega menu end-->

                    </div>
                </div>
            </div>

        </header>
        <!--header end-->

        <div class="navbar-house-keeping">
            {% block body %}{% endblock %}
        </div>
        <div id="user-floating-btn" class="fixed-action-btn toolbar hide-on-large-only	">
            <a class="btn-floating btn-large blue">
                <i class="fa fa-user" aria-hidden="true"></i>
            </a>
            <ul>

                {% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}
                    <li class="waves-effect waves-light">
                        <a href="{{ path('fos_user_security_logout') }}">
                            <i class="fa fa-sign-out" aria-hidden="true"></i>
                            Logout
                        </a>
                    </li>
                    <li class="waves-effect waves-light">
                        <a href="{{ path('fos_user_profile_show') }}">
                            <i class="fa fa-user" aria-hidden="true"></i>
                            Profile
                        </a>
                    </li>
                    <li class="waves-effect waves-light">
                        <a href="{{ path('fos_user_change_password') }}">
                            <i class="fa fa-cog" aria-hidden="true"></i>
                            Settings
                        </a>
                    </li>
                  {% else %}
                    <li class="waves-effect waves-light">
                        <a href="{{ path('fos_user_registration_register') }} ">
                            <i class="fa fa-user-plus" aria-hidden="true"></i>
                            Register
                        </a>
                    </li>
                    <li class="waves-effect waves-light">
                        <a href="{{ path('fos_user_security_login') }}">
                            <i class="fa fa-sign-in" aria-hidden="true"></i>
                            Login
                        </a>
                    </li>
                  {% endif %}
            </ul>
        </div>
                
        <footer class="footer footer-four">
            <div class="primary-footer brand-bg text-center">
                <div class="container">

                  <ul class="social-link tt-animate ltr mt-20">
                    <li><a href="#"><i class="fa fa-facebook"></i></a></li>
                    <li><a href="#"><i class="fa fa-twitter"></i></a></li>
                    <li><a href="#"><i class="fa fa-tumblr"></i></a></li>
                    <li><a href="#"><i class="fa fa-linkedin"></i></a></li>
                    <li><a href="#"><i class="fa fa-dribbble"></i></a></li>
                    <li><a href="#"><i class="fa fa-instagram"></i></a></li>
                    <li><a href="#"><i class="fa fa-rss"></i></a></li>
                  </ul>

                  <hr class="mt-15">

                  <div class="row">
                    <div class="col-md-12">
                          <div class="footer-logo">
                            <img src="{{ asset('new-theme/img/logo-white.png') }}" alt="">
                          </div>

                          <span class="copy-text">Copyright &copy; 2016 <a href="#">Materialize</a> &nbsp; | &nbsp;  All Rights Reserved &nbsp; | &nbsp;  Designed By <a href="#">TrendyTheme</a></span>
                          <div class="footer-intro">
                            <p>Penatibus tristique velit vestibulum adipiscing habitant aenean feugiat at condimentum aptent odio orci vulputate hac mollis a.Vestibulum adipiscing gravida justo a ac euismod vitae.</p>
                          </div>
                    </div><!-- /.col-md-12 -->
                  </div><!-- /.row -->
                </div><!-- /.container -->
            </div><!-- /.primary-footer -->

            <div class="secondary-footer brand-bg darken-2 text-center">
                <div class="container">
                    <ul>
                      <li><a href="#">Home</a></li>
                      <li><a href="#">About us</a></li>
                      <li><a href="#">Services</a></li>
                      <li><a href="#">Portfolio</a></li>
                      <li><a href="#">Contact us</a></li>
                    </ul>
                </div><!-- /.container -->
            </div><!-- /.secondary-footer -->
        </footer>

        <!-- Preloader -->
        <div id="preloader">
          <div class="preloader-position"> 
            <img src="{{ asset('new-theme/img/labado-logo.png') }}" alt="logo" >
            <div class="progress">
              <div class="indeterminate"></div>
            </div>
          </div>
        </div>
        <!-- End Preloader -->

        {% include 'fail.html.twig' %}

        <!-- jQuery -->
        <script src="{{ asset('new-theme/js/jquery-2.1.3.min.js') }}"></script>
        <script src="{{ asset('new-theme/bootstrap/js/bootstrap.min.js') }}"></script>
        <script src="{{ asset('new-theme/materialize/js/materialize.min.js') }}"></script>
        <script src="{{ asset('new-theme/js/menuzord.js') }}"></script>
        <script src="{{ asset('new-theme/js/bootstrap-tabcollapse.min.js') }}"></script>
        <script src="{{ asset('new-theme/js/jquery.easing.min.js') }}"></script>
        <script src="{{ asset('new-theme/js/jquery.sticky.min.js') }}"></script>
        <script src="{{ asset('new-theme/js/smoothscroll.min.js') }}"></script>
        <script src="{{ asset('new-theme/js/imagesloaded.js') }}"></script>
        <script src="{{ asset('new-theme/js/jquery.stellar.min.js') }}"></script>
        <script src="{{ asset('new-theme/js/jquery.inview.min.js') }}"></script>
        <script src="{{ asset('new-theme/js/jquery.shuffle.min.js') }}"></script>
        <script src="{{ asset('new-theme/owl.carousel/owl.carousel.min.js') }}"></script>
        <script src="{{ asset('new-theme/flexSlider/jquery.flexslider-min.js') }}"></script>
        <script src="{{ asset('new-theme/magnific-popup/jquery.magnific-popup.min.js') }}"></script>
        <script src="{{ asset('new-theme/js/scripts.js') }}"></script>

        <!-- RS5.0 Core JS Files -->
        <script src="{{ asset('new-theme/revolution/js/jquery.themepunch.tools.min.js') }}"></script>
        <script src="{{ asset('new-theme/revolution/js/jquery.themepunch.revolution.min.js') }}"></script>
        <script src="{{ asset('js/main.js') }}"></script>

        {% block javascripts %} {% endblock %}

    </body>
</html>
